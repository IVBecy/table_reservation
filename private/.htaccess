Options -Indexes

RewriteEngine On

ErrorDocument 404 /errors/404.html
ErrorDocument 403 /errors/403.html
ErrorDocument 500 /errors/500.html

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

<Files *.json>
  Require all denied
</Files>

<Files *.py>
    Require all denied
</Files>